<div id="menubar" >
				<ul id="mainmenu" >
						<li class="left"></li>
				<% String[] tabs={"Home","Ministry","Enter Stats","View Reports"};  %>
				<% String[] tabValues={"Home?action=home","Home?action=index&module="+(session.getValue("lastClass")==null?"location":(String)session.getValue("lastClass")),"Stat","Report"};  %>
				<% String[] tabModules={"home","skipthis","stat","report"};  %>
				<% for(int i=0;i<tabs.length;i++){ 
					if(module.equalsIgnoreCase(tabModules[i])||(tabs[i].equals("Ministry")&&searchModules.contains(majorModule))){%>
						
						<li id="tab_<%=i%>" class="active">
						<%=tabs[i]%>
				
						
					</li>
					<%}else{ %>
					<li id="tab_<%=i%>" class="inactive">
						<A href="<%=tabValues[i]%>"><%=tabs[i]%></A>
					</li>
					<%} %>
				<%}%>
					<li class="right"></li>
				</ul>
				<ul id="submenu" >
								<%if(searchModules.contains(module)){ %>
								<li class="ministry">&nbsp;</li>
								<% String[] minorTabs={"Location","People","Teams"};  
								String[] minorTabValues={"location","person","team"};  
								for(int j=0;j<minorTabs.length;j++){ %>
								<li id="minor_tab_<%=j%>" class="<%=(minorTabValues[j].equals(module))?"active":"" %>" >
									<%if (!minorTabValues[j].equals(module)){%>
										<A href="Home?action=index&module=<%=minorTabValues[j]%>"><%=minorTabs[j]%></A>
										<%}else{ %>
										<%=minorTabs[j]%>
										<%}%>
								</li>
								<%}
								%>
								<%} else if (module.equals("report")) { %>
								<li class="report">&nbsp;</li>
								<% String[] minorTabs={"Success Criteria","Ministry Rollcall"};  %>
								<% 
								String[] minorTabValues={"/servlet/Report?action=index&isMuster=false","/servlet/Report?action=index&isMuster=true"}; 
								 for(int j=0;j<minorTabs.length;j++){ %>
								<li id="minor_tab_<%=j%>"  >
									<A href="<%=minorTabValues[j]%>"><%=minorTabs[j]%></A>
								</li>
								<%}
								}
								%>
						</ul>
			</div>